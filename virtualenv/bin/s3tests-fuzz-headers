#!/root/s3-tests/virtualenv/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 's3tests','console_scripts','s3tests-fuzz-headers'
__requires__ = 's3tests'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('s3tests', 'console_scripts', 's3tests-fuzz-headers')()
    )
